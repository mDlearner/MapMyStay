<% layout('/layouts/boilerplate.ejs')%>
<body>
    <form action="/listings/new" method="get"><button class="btn">New Listing</button></form>
    <ul>
        <% for( list of lists) { %>
            <li class="li">
                <a href="/listings/<%= list._id %>">
                    <%= list.title %>
                </a>
            </li>
        <% } %>
    </ul>
</body>
